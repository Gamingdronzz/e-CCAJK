    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="?android:attr/actionBarSize"
        android:layout_marginLeft="@dimen/margin_medium"
        android:layout_marginRight="@dimen/margin_medium"
        android:layout_marginTop="@dimen/margin_medium"
        android:background="@color/colorWhite">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="?android:attr/actionBarSize"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:orientation="vertical"
                android:paddingLeft="@dimen/padding_large"
                android:paddingRight="@dimen/padding_large">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:paddingStart="@dimen/padding_small"
                    android:text="Circle"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/text_15" />

                <Spinner
                    android:id="@+id/spinner_grievance_circle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_medium"
                    android:background="@drawable/bg_spinner"
                    android:spinnerMode="dropdown" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:orientation="vertical"
                android:paddingLeft="@dimen/padding_large"
                android:paddingRight="@dimen/padding_large">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:paddingStart="@dimen/padding_small"
                    android:text="Grievance Type"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/text_15" />

                <Spinner
                    android:id="@+id/spinner_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_medium"
                    android:background="@drawable/bg_spinner"
                    android:spinnerMode="dropdown" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:orientation="vertical"
                android:paddingLeft="@dimen/padding_large"
                android:paddingRight="@dimen/padding_large">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:paddingStart="@dimen/padding_small"
                    android:text="Grievance Submitted By"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/text_15" />

                <Spinner
                    android:id="@+id/spinner_submitted_by"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_medium"
                    android:background="@drawable/bg_spinner"
                    android:spinnerMode="dropdown" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <LinearLayout
                android:id="@+id/layout_radio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_small"
                    android:paddingLeft="@dimen/padding_large"
                    android:text="Upload Using"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/text_12" />

                <RadioGroup
                    android:id="@+id/groupNumberType"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/padding_medium">

                    <RadioButton
                        android:id="@+id/radioButtonPensioner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checked="true"
                        android:text="Pensioner Code" />

                    <RadioButton
                        android:id="@+id/radioButtonHR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="HR Number" />

                    <RadioButton
                        android:id="@+id/radioButtonStaff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Staff Number" />

                </RadioGroup>

            </LinearLayout>


            <android.support.design.widget.TextInputLayout
                android:id="@+id/text_input_pensioner_code"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_min"
                android:background="@color/colorWhite"
                android:padding="@dimen/padding_small">

                <AutoCompleteTextView
                    android:id="@+id/autocomplete_pcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/padding_large"
                    android:hint="Pensioner Code"
                    android:inputType="number"
                    android:maxLength="15"
                    android:singleLine="true" />

            </android.support.design.widget.TextInputLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <android.support.design.widget.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:background="@color/colorWhite"
                android:padding="@dimen/padding_small">

                <AutoCompleteTextView
                    android:id="@+id/autocomplete_mobile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/padding_large"
                    android:hint="Mobile Number"
                    android:inputType="number"
                    android:maxLength="10"
                    android:singleLine="true" />

            </android.support.design.widget.TextInputLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <android.support.design.widget.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:background="@color/colorWhite"
                android:padding="@dimen/padding_small">

                <AutoCompleteTextView
                    android:id="@+id/autocomplete_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/padding_large"
                    android:hint="E-Mail"
                    android:inputType="textEmailAddress"
                    android:singleLine="true" />

            </android.support.design.widget.TextInputLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <android.support.design.widget.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_small"
                android:background="@color/colorWhite"
                android:padding="@dimen/padding_small">

                <EditText
                    android:id="@+id/edittext_details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawablePadding="@dimen/padding_large"
                    android:hint="Grievance Details"
                    android:singleLine="false" />

            </android.support.design.widget.TextInputLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="3dp"
                android:layout_marginTop="@dimen/margin_medium"
                android:background="@color/colorLightGray" />

            <TextView
                android:id="@+id/textview_selected_file_count_grievance"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/margin_large"
                android:background="@color/colorOffWhite"
                android:drawablePadding="@dimen/padding_medium"
                android:gravity="center"
                android:padding="@dimen/padding_medium"
                android:text="0 FILE SELECTED"
                android:textAlignment="center"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/text_15"
                android:textStyle="bold" />

            <android.support.v7.widget.RecyclerView
                android:id="@+id/recycler_view_selected_images"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>

    </ScrollView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/imageButton_removeAllFiles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button_submit"
        android:layout_alignParentStart="true"
        android:layout_margin="@dimen/margin_medium"
        android:tint="@color/colorWhite"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_close_black_24dp"
        tools:targetApi="m" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/button_attach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button_submit"
        android:layout_alignParentEnd="true"
        android:layout_margin="@dimen/margin_medium"
        android:tint="@color/colorWhite"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_attach_file_black_24dp"
        tools:targetApi="m" />

    <com.robertlevonyan.views.customfloatingactionbutton.FloatingActionLayout
        android:id="@+id/customFABL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button_submit"
        android:layout_margin="20dp"
        android:layout_marginTop="16dp"
        android:background="@color/colorAccent"
        app:fabColor="@color/colorPrimaryLight"
        app:fabType="circle">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:tint="@color/colorWhite"
                app:srcCompat="@drawable/ic_attach_file_black_24dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Add"
                android:textColor="@color/colorWhite" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="File"
                android:textColor="@color/colorWhite" />

        </LinearLayout>

    </com.robertlevonyan.views.customfloatingactionbutton.FloatingActionLayout>


    <com.robertlevonyan.views.customfloatingactionbutton.FloatingActionLayout
        android:id="@+id/customFABR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button_submit"
        android:layout_alignParentEnd="true"
        android:layout_margin="20dp"
        android:layout_marginTop="16dp"
        android:background="@color/colorAccent"
        app:fabColor="@color/colorPrimaryLight"
        app:fabType="circle">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:tint="@color/colorWhite"
                app:srcCompat="@drawable/ic_close_black_24dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Remove"
                android:textColor="@color/colorWhite" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="All"
                android:textColor="@color/colorWhite" />

        </LinearLayout>

    </com.robertlevonyan.views.customfloatingactionbutton.FloatingActionLayout>

    <Button
        android:id="@+id/button_submit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/custom_button"
        android:text="Submit"
        android:textColor="@color/colorWhite"
        android:textSize="@dimen/text_18"
        android:textStyle="bold" />

</RelativeLayout>


